{
    "name": "vandergangwatches",
    "version": "1.0.0",
    "description": "A watch order app",
    "main": "./dist/public/main.js",
    "scripts": {
        "start": "npm run serve",
        "start:dev": "npm run serve:dev",
        "build:webpack": "webpack --config webpack.config.js && cp ./build/bundle.js ./dist/public",
        "build:dev": "npm run build-ts && npm run lint && npm run copy-static-assets",
        "serve": "node dist/server.js",
        "serve:dev": "pm2 start run serve",
        "watch-node": "npx supervisor dist/server.js",
        "watch-node-inspect": "nodemon --config nodemon.json --inspect dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node,WebPack\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "build-ts": "tsc",
        "watch-ts": "tsc -w"
    },
    "keywords": [
        "typescript",
        "course"
    ],
    "author": "Gideon Nimoh",
    "license": "ISC",
    "devDependencies": {
        "@types/connect-redis": "^0.0.18",
        "@types/express-session": "^1.17.4",
        "@types/jquery": "^3.5.14",
        "@types/morgan": "^1.9.3",
        "@types/node": "^16.11.7",
        "ts-loader": "^9.2.6",
        "webpack-cli": "^5.0.0"
    },
    "dependencies": {
        "@types/concurrently": "^6.4.0",
        "@types/config": "^0.0.41",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/ioredis": "^4.28.5",
        "@types/logger": "^0.0.0",
        "axios": "^0.24.0",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^6.4.0",
        "config": "^3.3.7",
        "connect-redis": "^6.0.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "envalid": "^7.3.0",
        "express": "^4.17.1",
        "express-session": "^1.17.2",
        "helmet": "^4.6.0",
        "ioredis": "^4.28.2",
        "jspdf": "^2.5.1",
        "logger": "^0.0.1",
        "moment": "^2.29.4",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.15",
        "pm2": "^5.2.0",
        "qs": "^6.10.2",
        "redis": "^3.1.2",
        "supervisor": "^0.12.0",
        "ts-node": "^10.7.0",
        "tsconfig-paths": "^3.13.0",
        "typescript": "^4.4.4",
        "webpack": "^5.64.1",
        "webpack-dev-server": "^4.5.0",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.6.1"
    }
}
